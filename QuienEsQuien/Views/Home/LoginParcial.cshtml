@using QuienEsQuien.Models
@model Usuarios
@{
    ViewBag.Title = "LoginParcial";
}

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/Cosa.css" rel="stylesheet" />
</head>
<body class="w3-black">
    <div class="w3-padding-large" id="main">
        <form action="@Url.Action("Login", "Home")" method="get">
            @ViewBag.NoUserLogin <br /> <br />
            Nombre: @Html.TextBoxFor(x => x.Nombre, new { type = "string" }) <br />
            Contraseña: @Html.PasswordFor(x => x.Contraseña, new { type = "string" }) <br /> <br />

            <button>Siguiente</button>
        </form>
        
    </div>
</body>